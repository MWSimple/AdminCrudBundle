
    /**
{% block phpdoc_method_header %}
     * Exporter {{ entity }}.
{% endblock phpdoc_method_header %}
     *
{% block phpdoc_method_annotations %}
{% if 'annotation' == format %}
     * @Route("/exporter/{format}", name="{{ route_name_prefix }}_export")
{% endif %}
{% endblock phpdoc_method_annotations %}
     */
{% block method_definition %}
    public function getExporter($format, Request $request)
{% endblock method_definition %}
    {
{% block method_body %}
        $this->config['filterType'] = {{ entity_class }}FilterType::class;
        $response = parent::exportCsvAction($format, $request, false);
{% endblock method_body %}

{% block method_return %}
        return $response;
{% endblock method_return %}
    }
