{% extends 'MWSimpleAdminCrudBundle::layout.html.twig' %}

{% block title %}
{{ parent() }} - {{ 'views.show.show'|trans({'%entity%': config.entityName}, 'MWSimpleAdminCrudBundle') }}
{% endblock %}

{% block page %}
<div class="well well-lg">
    <h1>{{ config.entityName }}</h1>
    <form class="form-horizontal">
        <fieldset>                <div class="form-group">
                    <label class="control-label">Id</label>
                    <div class="form-control">                        <span class="input-xlarge uneditable-input">{{ entity.id }}</span>                    </div>
                </div>                <div class="form-group">
                    <label class="control-label">Booleano</label>
                    <div class="form-control">                        <span class="input-xlarge uneditable-input">{{ entity.booleano }}</span>                    </div>
                </div>                <div class="form-group">
                    <label class="control-label">Entero</label>
                    <div class="form-control">                        <span class="input-xlarge uneditable-input">{{ entity.entero }}</span>                    </div>
                </div>                <div class="form-group">
                    <label class="control-label">Smallentero</label>
                    <div class="form-control">                        <span class="input-xlarge uneditable-input">{{ entity.smallEntero }}</span>                    </div>
                </div>                <div class="form-group">
                    <label class="control-label">Bigentero</label>
                    <div class="form-control">                        <span class="input-xlarge uneditable-input">{{ entity.bigEntero }}</span>                    </div>
                </div>                <div class="form-group">
                    <label class="control-label">Cadena</label>
                    <div class="form-control">                        <span class="input-xlarge uneditable-input">{{ entity.cadena }}</span>                    </div>
                </div>                <div class="form-group">
                    <label class="control-label">Texto</label>
                    <div class="form-control">                        <span class="input-xlarge uneditable-input">{{ entity.texto }}</span>                    </div>
                </div>                <div class="form-group">
                    <label class="control-label">Fechatiempo</label>
                    <div class="form-control">                        <span class="input-xlarge uneditable-input">{{ entity.fechatiempo|date('Y-m-d H:i:s') }}</span>                    </div>
                </div>                <div class="form-group">
                    <label class="control-label">Dechatiempoz</label>
                    <div class="form-control">                        <span class="input-xlarge uneditable-input">{{ entity.dechatiempoz|date('Y-m-d') }}</span>                    </div>
                </div>                <div class="form-group">
                    <label class="control-label">Fecha</label>
                    <div class="form-control">                        <span class="input-xlarge uneditable-input">{{ entity.fecha|date('Y-m-d') }}</span>                    </div>
                </div>                <div class="form-group">
                    <label class="control-label">Tiempo</label>
                    <div class="form-control">                        <span class="input-xlarge uneditable-input">{{ entity.tiempo|date('H:i:s') }}</span>                    </div>
                </div>                <div class="form-group">
                    <label class="control-label">Numerodecimal</label>
                    <div class="form-control">                        <span class="input-xlarge uneditable-input">{{ entity.numerodecimal }}</span>                    </div>
                </div>                <div class="form-group">
                    <label class="control-label">Numeroconcoma</label>
                    <div class="form-control">                        <span class="input-xlarge uneditable-input">{{ entity.numeroconcoma }}</span>                    </div>
                </div>
        </fieldset>
    </form>
</div>
<div class="col-lg-9">
    <div class="float-left col-lg-3">
        <a class="btn btn-info col-lg-11" href="{{ path(config.index) }}">
            {{ 'views.recordactions.backtothelist'|trans({}, 'MWSimpleAdminCrudBundle') }}
        </a>
    </div>
    <div class="float-left col-lg-3">
        <a class="btn btn-primary col-lg-11" href="{{ path(config.edit, { 'id': entity.id }) }}">
            {{ 'views.recordactions.edit'|trans({'%entity%': config.entityName}, 'MWSimpleAdminCrudBundle') }}
        </a>
    </div>
    <div class="float-left col-lg-3">{{ form(delete_form) }}</div>
</div>
{% endblock %}