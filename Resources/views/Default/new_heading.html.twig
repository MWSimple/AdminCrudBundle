{% block panelheading %}
    <div class="float-left">
        <i class="glyphicon glyphicon-plus"></i>
        {{ 'views.new.creation'|trans({'%entity%': config.entityName}, 'MWSimpleAdminCrudBundle') }}
    </div>
{% endblock %}
{% block buttons %}
    <div class="float-right">
        <div class="btn-group">
            {% if config.index is defined %}
                <a class="btn-sm btn-default glyphicon glyphicon-th-list tooltips" href="{{ path(config.index) }}" title="" rel="tooltip" data-original-title="{{ 'views.recordactions.backtothelist'|trans({}, 'MWSimpleAdminCrudBundle') }}"></a>
            {% endif %}
        </div>
    </div>
{% endblock %}