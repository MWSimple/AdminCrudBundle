{
    "name": "mwsimple/admin-crud",
    "type": "symfony-bundle",
    "description": "Admin Crud Bundle generator for Symfony applications",
    "keywords": ["admin", "crud", "backend", "generator"],
    "homepage": "https://github.com/MWSimple/AdminCrudBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Gonzalo Alonso",
            "email": "gonkpo@gmail.com"
        }
    ],
    "require": {
        "symfony/framework-bundle": "~2.8 || ~3.0",
        "sensio/generator-bundle": "~3.0",
        "twig/extensions": "^1.3",
        "knplabs/knp-paginator-bundle": "~2.5",
        "lexik/form-filter-bundle": "~5.0",
        "knplabs/knp-menu": "~2.1",
        "knplabs/knp-menu-bundle": "~2.1",
        "sonata-project/exporter": "~1.4",
        "mwsimple/scdatetimepicker-bundle": "^2.0",
        "liip/imagine-bundle": "^1.5",
        "vich/uploader-bundle": "^1.1",
        "egeloen/ckeditor-bundle": "^6.0",
        "tetranz/select2entity-bundle": "^2.9"
    },
    "scripts": {
        "symfony-scripts": [
            "Ivory\\CKEditorBundle\\Composer\\CKEditorScriptHandler::install"
        ],
        "post-install-cmd": [
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": { "MWSimple\\Bundle\\AdminCrudBundle\\": "" },
        "exclude-from-classmap": ["/Tests/"]
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-version30": "4.0.x-dev"
        }
    }
}
